  "@comment scripts": {
    "dev": "Default development start command.",
    "dev:clean": "Run dev server without cache.",
    "dev:demo": "Run dev demo mode.",
    "build:typecheck": "Build production app with typecheck.",
    "build": "Current default build command. Vite doesn't check types on it's own, so while errors aren't cleaned from this app, run this.",
    "preview": "Default vite run preview production app. Port must be specified.",
    "aboutdevbuildandpreviewmodes": "https://vitejs.dev/guide/env-and-mode.html#modes",
    "format": "Format files according to prettier rules, but not lint.",
    "lint": "Check for linting and formatting errors with Prettier and ESlint",
    "lint:fix": "Same as above but also fix.",
    "lint:quiet": "Report only ESlint errors, not warnings.",
    "check:types": "Run tsc to check types.",
    "check:format": "Run prettier to check formatting, not ESLint linting.",
    "validate:all": "Runs all checks.",
    "test": "Runs all tests.",
    "test:watch": "Run tests related to changed files based on uncommitted files and keeps watch.",
    "test:coverage": "Check app test coverage.",
    "cy:run": "Runs all JS cypress tests.",
    "cy:open": "Opens cypress."
  },
  "scripts": {
    "dev": "vite",
    "dev:clean": "npm run dev --force",
    "dev:reset": "rm -rf node_modules && rm -f package-lock.json && rm -f yarn.lock && yarn && yarn dev:clean",
    "dev:demo": "yarn dev --REACT_APP_STAGE=demo",
    "build:production": "yarn install && ./build.sh production",
    "build:sourcemaps": "./build.sh",
    "build:typecheck": "tsc --noEmit && vite build",
    "build": "vite build",
    "preview": "vite preview --port 2000",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|json|ts|tsx)\"",
    "lint": "prettier --check 'src/**/*.{ts,tsx}' && eslint src",
    "lint:fix": "prettier --write 'src/**/*.{ts,tsx}' && eslint --fix src",
    "lint:quiet": "yarn lint --quiet",
    "check:types": "tsc --pretty --skipLibCheck --noEmit",
    "check:format": "prettier --check \"**/*.+(js|json|ts|tsx)\"",
    "validate:all": "yarn check:types && yarn check:format && yarn lint:quiet",
    "test": "jest",
    "test:watch": "yarn test --watch",
    "test:coverage": "yarn test --coverage",
    "cy:run": "cypress run --spec 'cypress/**/*.ts'",
    "cy:open": "cypress open",
    "prepare": "husky install"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "verbose": true,
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jsdom",
    "preset": "ts-jest",
    "globals": {
      "extensionsToTreatAsEsm": [
        ".ts",
        ".js"
      ],
      "ts-jest": {
        "isolatedModules": true,
        "useESM": true
      }
    },
    "transform": {
      "\\.(ts|js)x?$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs|cjs|ts|tsx)$"
    ],
    "resetMocks": true,
    "moduleNameMapper": {
      ".+\\.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "identity-obj-proxy",
      "\\.svg$": "<rootDir>/__mocks__/svg.js",
      "~(.*)$": "<rootDir>/src/$1"
    },
    "moduleDirectories": [
      "node_modules",
      "<rootDir>/node_modules",
      "."
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ],
    "testTimeout": 10000
  },
  "lint-staged": {
    "*.{js,ts,tsx,yaml,yml,json,md}": [
      "prettier --write"
    ],
    "*.{js,ts,tsx}": [
      "prettier --write",
      "eslint --fix --quiet"
    ]
  }
}
